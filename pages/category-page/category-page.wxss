/* pages/category-page/category-page.wxss */
.category-page {
  height: 100vh;
  padding: 20rpx;
  background-color: #f3f3f3;
  box-sizing: border-box;
  overflow: auto;

}
.title {
  padding: 20rpx;
  margin-bottom: 20rpx;
  border-radius: 12rpx;
  background-color: #fff;
}

.list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
 
}
.item {
  width: 48%;
}